{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "dom"],
    "baseUrl": "./",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": ["cypress", "node"],
    "module": "commonjs"
  },
  "include": [
    "cypress/**/*"
  ]
}
